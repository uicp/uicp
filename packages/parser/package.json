{
  "name": "@uicp/parser",
  "version": "0.1.0",
  "description": "UICP parser for extracting and rendering UI components from LLM output",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "uicp",
    "parser",
    "ui-components",
    "llm",
    "dynamic-ui"
  ],
  "author": "UICP Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/indigoai-us/uicp.git",
    "directory": "packages/parser"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    }
  }
}

